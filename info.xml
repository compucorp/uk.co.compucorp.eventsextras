<?xml version="1.0"?>
<extension key="uk.co.compucorp.eventsextras" type="module">
  <file>eventsextras</file>
  <name>CiviEvent Extras</name>
  <description>This extensions allows CiviCRM admin to config generic Event pages</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compuco.io</email>
  </maintainer>
  <releaseDate>2023-01-20</releaseDate>
  <version>2.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <comments>
    Supported CiviCRM versions  :
    - 5.51.3
  </comments>
  <civix>
    <namespace>CRM/EventsExtras</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_EventsExtras_Upgrader</upgrader>
</extension>
